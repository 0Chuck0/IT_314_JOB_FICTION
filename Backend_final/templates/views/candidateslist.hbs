    </div>
  </div>
    

 {{>header2}}


    <!-- HOME -->
    <section class="section-hero overlay inner-page bg-image" style="background-image: url('/images/hero_1.jpg');" id="home-section">
      <div class="container">
        <div class="row">
          <div class="col-md-7">
            <div class="new2">
              <h1 class="text-white font-weight-bold">Candidate list</h1>
            </div>
            <div class="custom-breadcrumbs">
              <!-- <a href="#">Home</a> <span class="mx-2 slash">/</span> -->
              <!-- <span class="text-white"><strong>Register</strong></span> -->
            </div>
          </div>
        </div>
      </div>
    </section>

    <section class="containerr">
          <!-- Sidenav -->
    <div class="toggle-btn" onclick="viewmenu()">
      <span></span>
      <span></span>
      <span></span>
  </div>
  <section class="containerr">
    <main class="container">
      <div class="candidates-listing-item">
        <h2 onclick="handle()">Showing Results</h2>
         {{#if registers}}
      {{#each registers}}
        <div class="list-grid-item mt-4 p-2">
          <div class="row">
            <div class="col-md-9">
              <div class="candidates-img float-left mr-4" id="azahar">
                <img src="{{profile}}" alt="" class="img-fluid d-block rounded">
              </div>
              <div class="candidates-list-desc job-single-meta  pt-2">
                <h5 class="mb-2 f-19">{{name}}</h5>
                <ul class="list-inline mb-0">
                  <li class="list-inline-item mr-4">
                    <p class="text-muted f-15 mb-0"><i class="mdi mdi-account mr-1"></i>{{field}}</p>
                  </li>
                  <!-- <li class="list-inline-item mr-4">
                      <p class="text-muted f-15 mb-0"><i class="mdi mdi-map-marker mr-1"></i>DAIICT</p>
                    </li> -->
                  <li class="list-inline-item">
                    <p class="text-muted f-15 mb-0"><i class="fa fa-briefcase" aria-hidden="true"></i> {{experience}} Year</p>
                  </li>
                </ul>
                <p class="text-muted mt-1 mb-0"><span class="boldspan">Skills : </span>{{technical_skills}}</p>
              </div>
            </div>
            <div class="col-md-3">
              <div class="candidates-list-fav-btn text-right">
                <div class="candidates-listing-btn mt-4">
                  <a href="/candidateprofile/{{email}}" target="_blank" class="btn btn-outline btn-sm">View
                    Profile</a>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-lg-11 offset-lg-1">
              <div class="candidates-item-desc">
              
                {{!-- <p class="text-muted mb-2 f-14">Lorem ipsum dolor, sit amet consectetur adipisicing elit. Rerum optio
                  debitis accusamus, hic adipisci sit alias quia similique, ea tempore tempora labore ipsum cum voluptatum
                  sed blanditiis delectus perspiciatis, pariatur consectetur consequuntur nihil dolores?</p> --}} 
              </div>
            </div>
          </div>
        </div>
         {{/each}}
        {{else}}
           <div style="text-align: center;">
                <p><h2>No applied candidates for this job posts yet</h2></p>
             </div>
            
        {{/if}}
        
      
  
        
      </div>
    </main>
  </section>
    
