    <script>
        function lidateName() {
            var nameInput = document.getElementById('candidate_project');
            var nameValue = nameInput.value.trim();

            // Regular expression to check if the value contains only alphabets and spaces
            var regex = /^[A-Za-z\s]+$/;

            // Check if the value matches the regular expression
            if (!regex.test(nameValue)) {
                document.getElementById('validproject').textContent = 'Name should contain only alphabets and spaces.';
                nameInput.setCustomValidity('Invalid name'); // Set custom validity to prevent form submission
            } else {
                document.getElementById('validproject').textContent = '';
