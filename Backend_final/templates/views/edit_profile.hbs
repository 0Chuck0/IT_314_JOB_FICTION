    <script>
        function func(event) {

            const fi = document.getElementById('candidate_image');
            const img = fi.value.split('.').pop().toLowerCase().toString();
            var imageExts = ['jpg', 'jpeg', 'png'];
            // Check if any file is selected. 
            if (fi.files.length > 0) {
                console.log(img)
                if (imageExts.indexOf(img) === -1) {
                    fi.value = null
                    document.getElementById("candidatephoto").src = 'images/photo.webp';
                    alert('Only jpg/jpeg and png files are allowed!');
                }
                if (imageExts.indexOf(img) !== -1) {
                    var image = URL.createObjectURL(event.target.files[0]);

                    document.getElementById("candidatephoto").src = image;
                }
            }

        }
    </script>
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            let marginzero = document.getElementById('another');

            window.addEventListener('click', function (event) {
                if (event.target.matches('#another')) {
                    // Clicking on the element toggles the 'active' class
                    marginzero.classList.toggle('active');
                } else {
                    // Clicking outside of the element removes the 'active' class
                    marginzero.classList.remove('active');
                }
            });

            // Prevent the click inside the element from closing immediately
            marginzero.addEventListener('click', function (event) {
                event.stopPropagation();
            });

        });

        document.addEventListener('DOMContentLoaded', function () {
            let marginzero = document.getElementById('zeromargin');

            window.addEventListener('click', function (event) {
