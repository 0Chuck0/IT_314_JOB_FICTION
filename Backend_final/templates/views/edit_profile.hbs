    <script>
        document.addEventListener('DOMContentLoaded', () => {
            console.log('DOM fully loaded');
            fetch('/Edit_profile')
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Network response was not ok');
                    }
                    return response.json();
                })
                .then(data => {
                    console.log('Data received:', data);
                    const resumeLink = data.data.resume_link;
                    console.log('Resume link:', resumeLink);
                    const resumeInput = document.getElementById('candidate_resume');

                    if (resumeLink && resumeInput) {
                        resumeInput.value = resumeLink;
                        console.log('Resume input value set:', resumeLink);
                    } else {
                        console.error('Unable to find resume input or link');
                    }
                })
                .catch(error => {
                    console.error('Error fetching data:', error);
                });
        });
    </script>


