            <div class="main-container container row">

                <div id="filter-container" class="container-fluid text-center filter-container">
                    <div class="row">
                        <div class="col-4 form-class">
                            <form id="filter-form" class="col-10" onchange="display_jobs()">
                                <div class="col">
                                  <h6 class="filter-button"> Serach </h6>
                                  <input type="text" name = "search" class="form-control form-control-lg" placeholder="Job title, Company...">
                                </div>
                                <% if (typeof search !== 'undefined') { %>
                                  <script>
                                    document.getElementsByName("search")[0].value = '<%= search %>';
                                  </script>
                                <% } %>
                                <div class="col">
                                    <h6 id="filter-button1" class="filter-button">Job Titles</h6>
                                    <div id="options1" class="options col-10">
                                        <input id="f1-opt1" type="checkbox" value="Software Engineer" name="job_titles">
                                                <label for="f1-opt1">Software Engineer</label>
                                                <br>
                                                <input id="f1-opt2" type="checkbox" value="Data Scientist" name="job_titles">
                                                <label for="f1-opt2">Data Scientist</label>
                                                <br>
                                                <input id="f1-opt3" type="checkbox" value="UI Developer" name="job_titles">
                                                <label for="f1-opt3">UI Developer</label>
                                                <br>
                                                <input id="f1-opt4" type="checkbox" value="Electrical Engineer" name="job_titles">
                                                <label for="f1-opt4">Electrical Engineer</label>

                                    </div>
                                </div>
                                <div class="col">
                                    <h6 id="filter-button7" class="filter-button">Experience</h6>
                                    <div class="row col-11 slider">
                                        <div class="col-3  range-min">
                                            <h6 class="range-ends">0 years</h6>
                                        </div>
                                        <div class="range col">
                                            <div class="range__content">

                                                <div class="range__slider">
                                                    <div class="range__slider-line" id="range-line"></div>
                                                </div>

                                                <div class="range__thumb" id="range-thumb">
                                                    <div class="range__value">
                                                        <span class="range__value-number"
                                                            id="range-number">50</span>
                                                    </div>
                                                </div>

                                                <input type="range" class="range__input" id="range-input" min="0"
                                                    max="100" value="50" step="10" name="experience">
                                            </div>
                                        </div>
                                        <div class="col-3 range-max">
                                            <h6 class="range-ends">5 years</h6>
                                        </div>
                                    </div>
                                </div>
                                <div class="col">
                                    <h6 id="filter-button2" class="filter-button">Locations</h6>
                                    <div id="options2" class="options col-10">
                                        <input id="f2-opt1" type="checkbox" value="Gurgaon" name="locations">
                                        <label for="f2-opt1">Gurgaon</label>
                                        <br>
                                        <input id="f2-opt2" type="checkbox" value="Bengaluru" name="locations">
                                        <label for="f2-opt2">Bengaluru</label>
                                        <br>
                                        <input id="f2-opt3" type="checkbox" value="Hyderabad" name="locations">
                                        <label for="f2-opt3">Hyderabad</label>
                                        <br>
                                        <input id="f2-opt4" type="checkbox" value="Pune" name="locations">
                                        <label for="f2-opt4">Pune</label>
                                        <br>
                                        <input id="f2-opt5" type="checkbox" value="Gandhinagar" name="locations">
                                        <label for="f2-opt5">Gandhinagar</label>

                                        <% if (typeof locations !== 'undefined') { %>
                                        <script>
                                              let x = document.getElementsByName("locations");
                                              for(let i in x){
                                                console.log(x[i].value);
                                                if(x[i].value == '<%= locations %>') x[i].checked = true;

                                              }
                                          </script>
                                        <% } %>

                                    </div>
                                </div>
                                <div class="col">
                                    <h6 id="filter-button3" class="filter-button">Work Modes</h6>
                                    <div id="options3" class="options col-10">
                                        <input id="f3-opt1" type="checkbox" value="Office" name="work_modes">
                                                <label for="f3-opt1">Office</label>
                                                <br>
                                                <input id="f3-opt2" type="checkbox" value="Remote" name="work_modes">
                                                <label for="f3-opt2">Remote</label>
                                                <br>
                                                <input id="f3-opt3" type="checkbox" value="Hybrid" name="work_modes">
                                                <label for="f3-opt3">Hybrid</label>


                                                <% if (typeof work_modes !== 'undefined') { %>
                                                  <script>
                                                        let y = document.getElementsByName("work_modes");
                                                        for(let i in y){
                                                          console.log(y[i].value);
                                                          if(y[i].value == '<%= work_modes %>') y[i].checked = true;
          
                                                        }
                                                    </script>
                                                  <% } %>

                                    </div>
                                </div>

                                <div class="col">
                                    <h6 id="filter-button4" class="filter-button">Qualification</h6>
                                    <div id="options4" class="options col-10">
                                        <input id="f4-opt1" type="checkbox" value="BTech" name="educations">
                                                <label for="f4-opt1">Bachelors</label>
                                                <br>
                                                <input id="f4-opt2" type="checkbox" value="MTech" name="educations"> 
                                                <label for="f4-opt2">Masters</label>
                                                <br>
                                                <input id="f4-opt3" type="checkbox" value="PhD" name="educations">
                                                <label for="f4-opt3">PhD</label>
                                    </div>
                                </div>
                                <div class="col">
                                    <h6 id="filter-button5" class="filter-button">Companies</h6>
                                    <div id="options5" class="options col-10">
                                        <input id="f5-opt1" type="checkbox" value="Google" name="companies">
                                                <label for="f5-opt1">Google</label>
                                                <br>
                                                <input id="f5-opt2" type="checkbox" value="Microsoft" name="companies">
                                                <label for="f5-opt2">Microsoft</label>
                                                <br>
                                                <input id="f5-opt3" type="checkbox" value="Deshaw" name="companies">
                                                <label for="f5-opt3">D.E Shaw</label>
                                                <br>
                                                <input id="f5-opt4" type="checkbox" value="Sprinklr" name="companies">
                                                <label for="f5-opt4">Sprinklr</label>
                                    </div>
                                </div>
                                <div class="col">
                                    <h6 id="filter-button6" class="filter-button">Skills</h6>
                                    <div id="options6" class="options col-10">
                                        <input id="f6-opt1" type="checkbox" value="c++" name="skills">
                                        <label for="f6-opt1">C++</label>
                                        <br>
                                        <input id="f6-opt2" type="checkbox" value="python" name="skills">
                                        <label for="f6-opt2">Python</label>
                                        <br>
                                        <input id="f6-opt3" type="checkbox" value="java" name="skills">
                                        <label for="f6-opt3">Java</label>
                                        <br>
                                        <input id="f6-opt4" type="checkbox" value="AD" name="skills">
                                        <label for="f6-opt4">Android Development</label>
                                        <br>
                                        <input id="f6-opt4" type="checkbox" value="WD" name="skills">
                                        <label for="f6-opt4">Web Development</label>
                                    </div>
                                </div>

                                <div class="col">
                                    <h6 id="filter-button8" class="filter-button">Salary (per month)</h6>
                                    <div id="options8" class="options col-10">
                                        <input id="f8-opt3" type="radio" value='{"min":0,"max":25000}' name="salary">
                                        <label for="f8-opt1">₹0 - ₹25,000</label>
                                        <br>
                                        <input id="f8-opt3" type="radio" value='{"min":25000,"max":50000}' name="salary">
                                        <label for="f8-opt2">₹25,000 - ₹50,000</label>
                                        <br>
                                        <input id="f8-opt3" type="radio" value='{"min":50000,"max":100000}' name="salary">
                                        <label for="f8-opt3">₹50,000 - ₹1,00,000</label>
                                        <br>
                                        <input id="f8-opt3" type="radio" value='{"min":100000,"max":1000000}' name="salary">

                                        <label for="f8-opt3">> ₹1,00,000</label>
                                        <br>
                                        <input id="f8-opt3" type="radio" value="None" name="salary">
                                        <label for="f8-opt3">No range</label>
                                        

                                    </div>
                                </div>

                            </form>
                        </div>



                        <script id="job-template" type="text/x-handlebars-template">
                            <a href="/job_description/{{id}}" >

                                <div class="row jobs-card">
                                    <div class="col-2 company-logo">
                                        <img src="/images/Tesla.svg" alt="Company-logo" class="job-profile">
                                    </div>
                                    <div class="col job-detail">
                                        <h4>{{company}}</h4>
                                        <h3>{{job_title}}</h3>
                                        <p>{{location}}</p>
                                    </div>

                                    <div class="col-4 job-labels mt-3">
                                        <div class="row labels">
                                            <span href="#" class="label-a">₹{{salary}} a month</span>
                                        </div>
                                        <div class="row labels">
                                            <span href="#" class="label-c">{{work_mode}}</span>
                                        </div>
                                    </div>
                                </div>
                            </a>
                              </script>




                        <div id="jobListing-container" class="col container jobListing-container scroll">

                          <% for (let key in data) { %>
                            
                            <a href="/job_description/<%= data[key].id %>" >

                                <div class="row jobs-card">
                                    <div class="col-2 company-logo">
                                        <img src="/images/Tesla.svg" alt="Company-logo" class="job-profile">
                                    </div>
                                    <div class="col job-detail">
                                        <h4><%= data[key].company %></h4>
                                        <h3><%= data[key].job_title%></h3>
                                        <p><%= data[key].location%></p>
                                    </div>

                                    <div class="col-4 job-labels mt-3">
                                        <div class="row labels">
                                            <span href="#" class="label-a">₹<%= data[key].salary%> a month</span>
                                        </div>
                                        <div class="row labels">
                                            <span href="#" class="label-c"><%= data[key].work_mode%></span>
                                          </div>
                                        </div>
                                      </div>
                                    </a>
                                    <% } %>
                                  </div>
                                </div>
                </div>
            </div>
        </div>
</div>
</div>


